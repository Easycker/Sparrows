<mod_static>
<!静态文件模块/>
	<mime>
	  <!文件mime头设置，根据扩展名选择/>
		<mime dot=".txt" type="text/plain"/>
		<mime dot=".css" type="text/css"/>
		<mime dot=".js" type="text/javascript"/>
		<mime dot=".htm" type="text/html"/>
		<mime dot=".html" type="text/html"/>
		<mime dot=".gif" type="image/gif"/>
		<mime dot=".jpeg" type="image/jpeg"/>
		<mime dot=".jpg" type="image/jpeg"/>
		<mime dot=".png" type="image/png"/>
		<mime dot=".tar.gz" type="application/gzip"/>
		<mime dot=".iso" type="application/octet-stream"/>
	</mime>
	<default_mime>
	<!默认mime头/>
		<mime type="application/octet-stream"/>
	</default_mime>
	<root_dir>"/home/more/Documents/workspace/www"</root_dir>
	<!根目录/>
	<index_page>"index.html"</index_page>
	<!主页/>
	<buf_size>32768</buf_size>
	<!缓存大小,不小于4096/>
</mod_static>
