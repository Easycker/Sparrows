<responder>
	<mods>
		<mod_static path="mod_static.so" config="mod_static.xml">
			<dir>
				<dir path="/"/>
				<dir path="/button/"/>
			</dir>
			<type>
				<type dot=".htm"/>
				<type dot=".html"/>
				<type dot=".jpg"/>
				<type dot=".png"/>
				<type dot=".css"/>
				<type dot=".gif"/>
				<type dot=".js"/>
			</type>
		</mod_static>
	</mods>
</responder>
<io>
	<max_process>3000</max_process>
	<stable_process>5</stable_process>
	<http_port>8802</http_port>
	<cache_adr>localhost</cache_adr>
	<cache_port>8803</cache_port>
	<resv_buf>512</resv_buf>
	<lock_file>/tmp/io_lock</lock_file>
</io>